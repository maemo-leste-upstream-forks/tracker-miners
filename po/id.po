#: ../data/org.freedesktop.Tracker.Extract.gschema.xml.h:2
#: ../data/org.freedesktop.Tracker.Miner.Files.gschema.xml.h:2
#: ../data/org.freedesktop.Tracker.Writeback.gschema.xml.h:2
